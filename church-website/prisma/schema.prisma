datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Contact {
  id        Int      @id @default(autoincrement())
  name      String
  email     String
  message   String
  phone     String?  // <-- optional phone number
  createdAt DateTime @default(now())
}